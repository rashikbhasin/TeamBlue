<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cfit012.events_mini_project">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Add_activity" />
        <activity android:name=".Delete_activity" />
        <activity android:name=".Update_activity" />
        <activity android:name=".Read_activity" />
        <activity android:name=".City_activity" />
        <activity android:name=".Date_activity" />
        <activity android:name=".List_activity" />
        <activity android:name=".Upcoming_activity" />
        <activity android:name=".Completed_activity"></activity>
    </application>

</manifest>